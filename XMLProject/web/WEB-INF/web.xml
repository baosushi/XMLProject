<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>GetUniversityList</servlet-name>
        <servlet-class>com.servlets.GetUniversityList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DispatchServlet</servlet-name>
        <servlet-class>com.servlets.DispatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUniversityInfo</servlet-name>
        <servlet-class>com.servlets.GetUniversityInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrawlDataOfBlocksServlet</servlet-name>
        <servlet-class>com.servlets.CrawlDataOfBlocksServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUniversityList</servlet-name>
        <url-pattern>/GetUniversityList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrawlDataOfBlocksServlet</servlet-name>
        <url-pattern>/CrawlDataOfBlocksServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DispatchServlet</servlet-name>
        <url-pattern>/dispatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUniversityInfo</servlet-name>
        <url-pattern>/university-info</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>search.jsp</welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
